{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-airbnb"
  ],
  "rules": {
    "jsx-alignment": [
      true
    ],
    "jsx-max-props-per-line": [
      1,
      {
        "when": "always"
      }
    ],
    "jsx-wrap-multiline": [
      true
    ],
    "jsx-no-multiline-js": [
      false
    ],
    "jsx-no-lambda": [
      false
    ],
    "no-boolean-literal-compare": [
      true
    ],
    "no-console": [
      false
    ],
    "import-name": [
      false
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "allow-leading-underscore"
    ],
    "ordered-imports": [
      false
    ],
    "object-literal-sort-keys": [
      false
    ]
  },
  "jsRules": {},
  "rulesDirectory": []
}
